{
  "$model": "infections", 
	"$select": {
     "$fields": ["is_active", "form_version", "nhsn_reportable"],
     "criterias": ["criteria_id", "element_id"]
  },
  "$order": {"id": "$desc"},
  "$where": {"instance_id": 15}      
}

{
  "$model": "infections", 
	"$select": {
    "$fields": ["is_active", "form_version", "nhsn_reportable"],
    "criterias": ["criteria_id", "element_id"]
  },
  "$order": {"id": "$asc"},
  "$where": {"instance_id": 10},
  "$include": {
    "criterias": {
      "$where": {"criteria_id": "ham"},
      "$order": {"id": "$desc"},
      "$join": "$right"
    }
  }     
}

{
  "$model": "infections", 
	"$select": {
    "$fields": ["event_date", "schema_version", "form_version"]     
  },
  "$order": {"id": "$asc"},
  "$where": {"bed_id": 10},
  "$include": {
    "infection_rxs": {
      "$where": {"rx_id": 2},
      "$order": {"infection_id": "$asc"},
      "$join": "$left",
      "$select": ["rx_id", "infection_id"]      
    }
  }     
}

{
  "$model": "facilities", 
	"$select": {
    "$fields": ["name", "import_name", "description"]     
  },
  "$order": {"id": "$asc"},
  "$where": {"instance_id": 10},
  "$right_join": {
    "units": {
      "$on_field": "id",
      "$on_join_table_field": "facility_id",
      "$order": {"id": "$asc"}
    }
  }     
}

{
  "$model": "facilities", 
	"$select": {
      "$fields": ["name", "import_name", "description"]     
  },
  "$order": {"id": "$asc"},
  "$where": {"instance_id": 10},
  "$include": {
    "users": {
      "$include": ["instances"],
      "$where": {"id": "1"},
      "$order": {"id": "$desc"},
      "$join": "$right"
    }
  },
  "$right_join": {
    "units": {
       "$on_field": "id",
       "$on_join_table_field": "facility_id",
       "$order": {"id": "$asc"}
    }
  }     
}

{
  "$model": "facilities", 
	"$select": {
    "$fields": ["name", "import_name", "description"]   
  },
  "$order": {"id": "$asc"},
  "$where": {"instance_id": 10},
  "$include": {
    "users": {
      "$include": ["instances"],
      "$where": {"id": "1"},
      "$order": {"id": "$desc"},
      "$select": ["first_name", "last_name"],
      "$join": "$left"
    }
  },
  "$left_join": {
    "units": {
      "$on_field": "id",
      "$on_join_table_field": "facility_id",
      "$order": {"id": "$asc"}
    }
  }     
}

{
  "$model": "facilities", 
	"$select": {
    "$fields": ["name", "import_name", "description"]     
  },
  "$order": {"id": "$asc"},
  "$where": {"instance_id": 10},
  "$include": {
    "users": {
      "$include": ["instances"],
      "$where": {"id": "1"},
      "$order": {"id": "$desc"},
      "$select": ["first_name", "last_name", "username", "email_address"],
      "$join": "$inner",
      "$group": ["login_count"]
    }
  },
  "$inner_join": {
    "units": {
      "$on_field": "id",
      "$on_join_table_field": "facility_id",
      "$order": {"id": "$asc"}
    }
  }      
}

{
  "$model": "facilities", 
	"$select": {
    "$fields": ["name", "import_name", "description"]      
  },
  "$order": {"id": "$desc"},
  "$where": {"instance_id": 11},
  "$include": {
    "users": {
      "$include": ["instances"],
      "$where": {"id": "1"},
      "$order": {"id": "$desc"},
      "$select": ["first_name", "last_name", "username", "email_address"],
      "$join": "$inner",
      "$group": ["login_count"]
    }
  },
  "$inner_join": {
    "units": {
      "$on_field": "id",
      "$on_join_table_field": "facility_id",
      "$order": {"id": "$asc"},
      "$group": ["is_active"]
    }
  }      
}

{
  "$model": "facilities", 
	"$select": {
    "$fields": ["name", "import_name", "description"]     
  },
  "$order": {"id": "$descc"},
  "$where": {"instance_id": 11, "id": 1},
  "$include": {
    "users": {
      "$include": ["instances"],
      "$where": {"id": "2", "instance_id": 1},
      "$order": {"id": "$desc"},
      "$select": ["first_name", "last_name", "username", "email_address"],
      "$join": "$inner",
      "$group": ["login_count"]
    }
  },
  "$inner_join": {
    "units": {
      "$on_field": "id",
      "$on_join_table_field": "facility_id",
      "$order": {"id": "$asc"},
      "$group": ["is_active"]
    }
  }     
}

{
  "$model": "facilities", 
	"$select": 
      ["name", "import_name", "description"],
  "$order": {"id": "$descc"},
  "$where": {"instance_id": 11, "id": 1},
  "$include": {
    "users": {
      "$include": ["instances"],
      "$where": {"id": "2", "instance_id": 1},
      "$order": {"id": "$desc"},
      "$select": ["first_name", "last_name", "username", "email_address"],
      "$join": "$inner",
      "$group": ["login_count"]
    }
  },
  "$inner_join": {
    "units": {
      "$on_field": "id",
      "$on_join_table_field": "facility_id",
      "$order": {"id": "$asc"},
      "$group": ["is_active"]
    }
  }     
}
